@model IPagedList<EquiposTecnicosSN.Entities.Equipos.Info.Umdns>

@{
    ViewBag.Title = "Códigos UMDNS";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-primary">
            <div class="panel-heading"><i class="fa fa-search fa-fw"></i> Buscar UMDNS</div>
            <div class="panel-body">
                <div class="row">
                    <form id="formBuscarUmdns" method="get" data-nqn-ajax="true" action="@Url.Action("Index", "Umdns")" data-nqn-target="#umdnsList">
                        <div class="col-md-2">
                            <label class="control-label" style="margin-top: 5px;">Nombre Completo</label>
                        </div>
                        <div class="col-md-8">
                            <input type="text" id="searchUmdns" name="searchUmdns" class="form-control text-box single-line" />
                        </div>
                        <div class="col-md-2">
                            <input id="buscarSector" type="submit" value="Buscar" class="btn btn-primary" />
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading"><i class="fa fa-tags fa-fw"></i> Listado de @ViewBag.Title</div>
            <div class="panel-body">

                <div class="row">
                    <div class="col-md-12">
                        <div id="umdnsList">
                            @Html.Partial("_UmdnsList", Model)
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
