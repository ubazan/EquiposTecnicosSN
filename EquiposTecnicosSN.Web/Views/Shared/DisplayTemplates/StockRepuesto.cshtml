@model EquiposTecnicosSN.Entities.Mantenimiento.StockRepuesto

<div class="panel panel-@(ViewBag.PanelStyle != null ? ViewBag.PanelStyle : "default")">
    <div class="panel-heading"><i class="fa fa-gears fa-fw"></i> @ViewBag.Title</div>
    <div class="panel-body">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label>
                        @Html.DisplayNameFor(model => model.CantidadDisponible)
                    </label>
                    <p class="form-control-static">
                        @Html.DisplayFor(model => model.CantidadDisponible)
                    </p>
                </div>
            </div>

            <div class="col-md-6">
                <div class="form-group">
                    <label>
                        @Html.DisplayNameFor(model => model.Repuesto)
                    </label>
                    <p class="form-control-static">
                        @Html.DisplayFor(model => model.Repuesto.Nombre)
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
