@model EquiposTecnicosSN.Entities.Mantenimiento.ChecklistMantenimientoPreventivo

<div class="panel panel-@(ViewBag.PanelStyle != null ? ViewBag.PanelStyle : "default" )">
    <div class="panel-heading"><i class="fa fa-list-alt fa-fw"></i> @ViewBag.Title</div>
    <div class="panel-body">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.ChecklistMantenimientoPreventivoId)

        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label>
                        @Html.DisplayNameFor(model => model.Nombre)
                    </label>
                    <p class="form-control-static">
                        @Html.DisplayFor(model => model.Nombre)
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
