@model EquiposTecnicosSN.Entities.Mantenimiento.Proveedor

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-@(ViewBag.PanelStyle != null ? ViewBag.PanelStyle : "default")">
            <div class="panel-heading"><i class="fa fa-truck fa-fw"></i> @ViewBag.Title</div>
            <div class="panel-body">
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                @Html.HiddenFor(model => model.ProveedorId)

                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            @Html.LabelFor(model => model.Nombre, htmlAttributes: new { @class = "control-label" })
                            <p class="form-control-static">

                                @Html.DisplayFor(model => model.Nombre, new { htmlAttributes = new { @class = "form-control-static" } })
                            </p>                        

                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            @Html.LabelFor(model => model.Tipo, htmlAttributes: new { @class = "control-label" })
                            <p class="form-control-static">

                                @Html.DisplayFor(model => model.Tipo, new { htmlAttributes = new { @class = "form-control-static" } })
                            </p>
                        </div>

                        <div class="form-group">
                            @Html.LabelFor(model => model.Telefono, htmlAttributes: new { @class = "control-label" })
                            <p class="form-control-static">

                                @Html.DisplayFor(model => model.Telefono, new { htmlAttributes = new { @class = "form-control-static" } })
                            </p>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            @Html.LabelFor(model => model.Email, htmlAttributes: new { @class = "control-label" })
                            <p class="form-control-static">

                                @Html.DisplayFor(model => model.Email, new { htmlAttributes = new { @class = "form-control-static" } })
                            </p>
                        </div>

                        <div class="form-group">
                            @Html.LabelFor(model => model.Website, htmlAttributes: new { @class = "control-label" })
                            <p class="form-control-static">
                                <a target="_blank" href="http://@Model.Website">@Model.Website</a>
                            </p>

                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        @Html.LabelFor(model => model.Direccion, htmlAttributes: new { @class = "control-label" })
                        <p class="form-control-static">

                            @Html.DisplayFor(model => model.Direccion, new { @class = "control-label-static" })
                        </p>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        @Html.LabelFor(model => model.Servicios, htmlAttributes: new { @class = "control-label" })
                        <p class="form-control-static">

                            @Html.DisplayFor(model => model.Servicios, new { @class = "control-label-static" })
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
