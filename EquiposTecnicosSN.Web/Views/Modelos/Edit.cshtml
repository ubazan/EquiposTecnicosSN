@model EquiposTecnicosSN.Entities.Equipos.Info.Modelo

@{
    ViewBag.Title = "Modelo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm())
{
    
    <div class="row">
        <div class="col-md-12">
            @Html.EditorFor(model => model)
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <a href="@Url.Action("Index", new { marcaId = Model.MarcaId })" class="btn btn-default"> Volver</a>
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </div>
    </div>
}